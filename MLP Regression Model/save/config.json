{
    "name": "MLPSpeechDecodingModel",
    "n_gpu": 1,
    "encoder_prenet_out_d": 256,
    "num_classes": 20,
    "dropout": 0.0,
    "enc_hidden_dim": 256,
    "timepoints": 96,
    "num_eeg_channels": 127,
    "epochs": 200,
    "batch_size": 16,
    "p_sample": 1.0,
    "validation_ratio": 0.2,
    "logging_file": "train.log",
    "autosave_seconds": 100000,
    "checkpointfile": "checkpoint_2024-02-06_00-45-31",
    "kmeans_folder": "..\\Kmeans of sounds",

    "optimizer": {
        "lr": 1E-4,
        "weight_decay": 0.01,
        "amsgrad": true
    },
    "lr_scheduler": {
        "step_size": 50,
        "gamma": 0.1
    }
}
