{
    "name": "MLPSpeechDecodingModel",
    "n_gpu": 1,
    "encoder_prenet_out_d": 512,
    "enc_hidden_dim": 128,
    "d_model": 512,
    "num_heads":4,
    "num_layers": 2,
    "dim_feedforward":1024,
    "num_classes": 20,
    "dropout": 0.1,
    "timepoints": 100,
    "num_eeg_channels": 127,
    "epochs": 200,
    "batch_size": 16,
    "p_sample": 1.0,
    "validation_ratio": 0.2,
    "logging_file": "train.log",
    "autosave_seconds": 100000,
    "checkpointfile": null,
    "kmeans_folder": "..\\Kmeans of sounds",

    "optimizer": {
        "lr": 1E-4,
        "weight_decay": 0.01,
        "amsgrad": false
    },
    "lr_scheduler": {
        "step_size": 50,
        "gamma": 0.1
    }
}
